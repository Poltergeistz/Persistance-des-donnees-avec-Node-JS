<%- include partials/header.ejs %>

    <div class="container">
        <div class="row">
            <% posts.forEach(function(post, index){ %>
                <h5>
                    <%= post.title %>
                </h5>
                <p>
                    <%= post.post %>
                </p>
                <span><%= post.author %></span>
                <span><%= post.date %></span>
                <a href="edit/<%= post._id %>">Modifier</a>
                <a href="delete/<%= post._id %>">Supprimer</a>
                <% }) %>
        </div>
        <a href="/post/new">Cr√©er un nouveau post</a>
    </div>
    








